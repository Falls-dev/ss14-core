<widgets:TargetingWidget
    xmlns="https://spacestation14.io"
    xmlns:widgets="clr-namespace:Content.Client._White.Targeting.Ui"
    Name="TargetingButton"
    VerticalExpand="True"
    VerticalAlignment="Bottom"
    HorizontalAlignment="Right">
    <Control HorizontalAlignment="Right">
        <TextureRect
            Name="TargetDoll"
            Stretch="KeepAspectCentered"
            SetSize="96 96">
            <PanelContainer
                SetSize="57 96"
                Margin="18 0 0 0"
                VerticalAlignment="Bottom"
                HorizontalAlignment="Left">
                <PanelContainer
                    SetSize="18 39"
                    Margin="0 7 0 0"
                    HorizontalAlignment="Left">
                    <TextureButton
                        Name="RightArmButton"
                        MinSize="18 30"
                        StyleClasses="TargetingButtonRightArm"
                        VerticalAlignment="Top">
                        <TextureRect
                            TexturePath="/Textures/White/Targeting/Doll/rightarm.png"
                            Stretch="KeepAspectCentered"
                            SetSize="12 24"
                            VerticalAlignment="Center"
                            HorizontalAlignment="Center"/>
                        <TextureRect
                            TexturePath="/Textures/White/Targeting/Doll/rightarm_hover.png"
                            Stretch="KeepAspectCentered"
                            SetSize="12 24"
                            VerticalAlignment="Center"
                            HorizontalAlignment="Center"
                            Visible="False"/>
                    </TextureButton>
                    <TextureButton
                        Name="RightHandButton"
                        MinSize="18 18"
                        VerticalAlignment="Bottom"
                        StyleClasses="TargetingButtonRightHand">
                        <TextureRect
                            TexturePath="/Textures/White/Targeting/Doll/righthand.png"
                            Stretch="KeepAspectCentered"
                            SetSize="12 12"
                            VerticalAlignment="Center"
                            HorizontalAlignment="Center"/>
                        <TextureRect
                            TexturePath="/Textures/White/Targeting/Doll/righthand_hover.png"
                            Stretch="KeepAspectCentered"
                            SetSize="12 12"
                            VerticalAlignment="Center"
                            HorizontalAlignment="Center"
                            Visible="False"/>
                    </TextureButton>
                </PanelContainer>
                <PanelContainer
                    SetSize="51 90"
                    Margin="0 64 0 0"
                    VerticalAlignment="Bottom"
                    HorizontalAlignment="Center">
                    <TextureButton
                        Name="HeadButton"
                        MinSize="33 27"
                        VerticalAlignment="Top"
                        HorizontalAlignment="Center"
                        StyleClasses="TargetingButtonHead">
                        <TextureRect
                            TexturePath="/Textures/White/Targeting/Doll/head.png"
                            Stretch="KeepAspectCentered"
                            SetSize="27 21"
                            VerticalAlignment="Center"
                            HorizontalAlignment="Center"/>
                        <TextureRect
                            TexturePath="/Textures/White/Targeting/Doll/head_hover.png"
                            Stretch="KeepAspectCentered"
                            SetSize="27 21"
                            VerticalAlignment="Center"
                            HorizontalAlignment="Center"
                            Visible="False"/>
                    </TextureButton>
                    <TextureButton
                        Name="ChestButton"
                        SetSize="33 36"
                        Margin="0 21 0 0"
                        VerticalAlignment="Top"
                        HorizontalAlignment="Center"
                        StyleClasses="TargetingButtonChest">
                        <TextureRect
                            TexturePath="/Textures/White/Targeting/Doll/torso.png"
                            Stretch="KeepAspectCentered"
                            SetSize="27 24"
                            VerticalAlignment="Center"
                            HorizontalAlignment="Center"/>
                        <TextureRect
                            TexturePath="/Textures/White/Targeting/Doll/torso_hover.png"
                            Stretch="KeepAspectCentered"
                            SetSize="27 24"
                            VerticalAlignment="Center"
                            HorizontalAlignment="Center"
                            Visible="False"/>
                    </TextureButton>
                    <PanelContainer
                        MinSize="45 45"
                        VerticalAlignment="Bottom"
                        HorizontalAlignment="Center">
                        <TextureButton
                            Name="StomachButton"
                            MinSize="33 28"
                            VerticalAlignment="Top"
                            HorizontalAlignment="Center"
                            StyleClasses="TargetingButtonStomach">
                            <TextureRect
                                TexturePath="/Textures/White/Targeting/Doll/stomach.png"
                                Stretch="KeepAspectCentered"
                                SetSize="33 22"
                                VerticalAlignment="Center"
                                HorizontalAlignment="Center"/>
                            <TextureRect
                                TexturePath="/Textures/White/Targeting/Doll/stomach_hover.png"
                                Stretch="KeepAspectCentered"
                                SetSize="33 22"
                                VerticalAlignment="Center"
                                HorizontalAlignment="Center"
                                Visible="False"/>
                        </TextureButton>
                        <PanelContainer
                            MinSize="24 36"
                            VerticalAlignment="Bottom"
                            HorizontalAlignment="Right">
                            <TextureButton
                                Name="LeftLegButton"
                                MinSize="18 33"
                                VerticalAlignment="Top"
                                HorizontalAlignment="Left"
                                StyleClasses="TargetingButtonButtonLeftLeg">
                                <TextureRect
                                    TexturePath="/Textures/White/Targeting/Doll/leftleg.png"
                                    Stretch="KeepAspectCentered"
                                    SetSize="12 27"
                                    VerticalAlignment="Center"
                                    HorizontalAlignment="Center"/>
                                <TextureRect
                                    TexturePath="/Textures/White/Targeting/Doll/leftleg_hover.png"
                                    Stretch="KeepAspectCentered"
                                    SetSize="12 27"
                                    VerticalAlignment="Center"
                                    HorizontalAlignment="Center"
                                    Visible="False"/>
                            </TextureButton>
                            <TextureButton
                                Name="LeftFootButton"
                                MinSize="24 12"
                                VerticalAlignment="Bottom"
                                StyleClasses="TargetingButtonButtonLeftFoot">
                                    <TextureRect
                                        TexturePath="/Textures/White/Targeting/Doll/leftfoot.png"
                                        Stretch="KeepAspectCentered"
                                        SetSize="18 9"
                                        VerticalAlignment="Center"
                                        HorizontalAlignment="Center"/>
                                    <TextureRect
                                        TexturePath="/Textures/White/Targeting/Doll/leftfoot_hover.png"
                                        Stretch="KeepAspectCentered"
                                        SetSize="18 9"
                                        VerticalAlignment="Center"
                                        HorizontalAlignment="Center"
                                        Visible="False"/>
                            </TextureButton>
                        </PanelContainer>
                        <PanelContainer
                            MinSize="24 36"
                            VerticalAlignment="Bottom"
                            HorizontalAlignment="Left">
                            <TextureButton
                                Name="RightLegButton"
                                MinSize="18 33"
                                VerticalAlignment="Top"
                                HorizontalAlignment="Right"
                                StyleClasses="TargetingButtonButtonRightLeg">
                                <TextureRect
                                    TexturePath="/Textures/White/Targeting/Doll/rightleg.png"
                                    Stretch="KeepAspectCentered"
                                    SetSize="12 27"
                                    VerticalAlignment="Center"
                                    HorizontalAlignment="Center"/>
                                <TextureRect
                                    TexturePath="/Textures/White/Targeting/Doll/rightleg_hover.png"
                                    Stretch="KeepAspectCentered"
                                    SetSize="12 27"
                                    VerticalAlignment="Center"
                                    HorizontalAlignment="Center"
                                    Visible="False"/>
                            </TextureButton>
                            <TextureButton
                                Name="RightFootButton"
                                MinSize="24 12"
                                VerticalAlignment="Bottom"
                                HorizontalAlignment="Center"
                                StyleClasses="TargetingButtonButtonRightFoot">
                                <TextureRect
                                    TexturePath="/Textures/White/Targeting/Doll/rightfoot.png"
                                    Stretch="KeepAspectCentered"
                                    SetSize="18 9"
                                    VerticalAlignment="Center"
                                    HorizontalAlignment="Center"/>
                                <TextureRect
                                    TexturePath="/Textures/White/Targeting/Doll/rightfoot_hover.png"
                                    Stretch="KeepAspectCentered"
                                    SetSize="18 9"
                                    VerticalAlignment="Center"
                                    HorizontalAlignment="Center"
                                    Visible="False"/>
                            </TextureButton>
                        </PanelContainer>
                    </PanelContainer>
                </PanelContainer>
                <PanelContainer
                    SetSize="18 39"
                    Margin="0 7 0 0"
                    HorizontalAlignment="Right">
                    <TextureButton
                        Name="LeftArmButton"
                        MinSize="18 30"
                        StyleClasses="TargetingButtonButtonLeftArm"
                        VerticalAlignment="Top">
                        <TextureRect
                            TexturePath="/Textures/White/Targeting/Doll/leftarm.png"
                            Stretch="KeepAspectCentered"
                            SetSize="12 24"
                            VerticalAlignment="Center"
                            HorizontalAlignment="Center"/>
                        <TextureRect
                            TexturePath="/Textures/White/Targeting/Doll/leftarm_hover.png"
                            Stretch="KeepAspectCentered"
                            SetSize="12 24"
                            VerticalAlignment="Center"
                            HorizontalAlignment="Center"
                            Visible="False"/>
                    </TextureButton>
                    <TextureButton
                        Name="LeftHandButton"
                        MinSize="18 18"
                        VerticalAlignment="Bottom"
                        StyleClasses="TargetingButtonButtonLeftHand">
                        <TextureRect
                            TexturePath="/Textures/White/Targeting/Doll/lefthand.png"
                            Stretch="KeepAspectCentered"
                            SetSize="12 12"
                            VerticalAlignment="Center"
                            HorizontalAlignment="Center"/>
                        <TextureRect
                            TexturePath="/Textures/White/Targeting/Doll/lefthand_hover.png"
                            Stretch="KeepAspectCentered"
                            SetSize="12 12"
                            VerticalAlignment="Center"
                            HorizontalAlignment="Center"
                            Visible="False"/>
                    </TextureButton>
                </PanelContainer>
            </PanelContainer>
        </TextureRect>
    </Control>
</widgets:TargetingWidget>
